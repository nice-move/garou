{
  "name": "garou",
  "version": "0.9.15",
  "description": "Do thing what heroes didn't do",
  "license": "MIT",
  "author": {
    "name": "Eric Chen"
  },
  "keywords": [
    "css",
    "eslint",
    "format",
    "formatter",
    "garou",
    "html",
    "javascript",
    "markdown",
    "nice-move",
    "prettier",
    "react",
    "scss",
    "sort",
    "styleguide",
    "stylelint",
    "typescript",
    "vue"
  ],
  "homepage": "https://www.npmjs.com/package/garou",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nice-move/garou.git"
  },
  "bugs": {
    "url": "https://github.com/nice-move/garou/issues"
  },
  "bin": {
    "garou": "dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "garou": "node dist/cli.mjs",
    "lint:staged": "nice-move lint staged",
    "lint:type": "tsc --noEmit",
    "prepare": "nice-move git hooks",
    "prepublishOnly": "pnpm run lint:staged && pnpm test",
    "presnapshot": "pnpm run build",
    "snapshot": "ava --fail-fast -u -w",
    "pretest": "pnpm run build",
    "test": "ava --fail-fast"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^8.44.0",
    "cheetor": "^0.13.0",
    "eslint-module-utils": "^2.12.1",
    "parse-gitignore": "^2.0.0",
    "vue-eslint-parser": "^10.2.0"
  },
  "devDependencies": {
    "@bring-it/cli": "^0.11.0",
    "@bring-it/npm": "^0.9.0",
    "@eslint/markdown": "^7.2.0",
    "@ianvs/prettier-plugin-sort-imports": "npm:@nice-move/prettier-plugin-sort-imports@^4.5.1",
    "@nice-move/all-in-web": "^0.6.16",
    "@nice-move/eslint-plugin-html": "0.1.1",
    "@stylistic/stylelint-plugin": "^4.0.0",
    "@vue/compiler-sfc": "^3.5.21",
    "@yarnpkg/lockfile": "^1.1.0",
    "ava": "^6.4.1",
    "eslint": "^9.35.0",
    "eslint-plugin-ava": "^15.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-class-members": "^1.21.0",
    "eslint-plugin-vue": "^10.4.0",
    "espree": "^10.4.0",
    "fs-chain": "^10.0.0",
    "globby": "^14.1.0",
    "micromatch": "^4.0.8",
    "object-traversal": "^1.0.1",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "postcss-less": "^6.0.0",
    "postcss-markdown": "^1.3.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "rehype": "^13.0.2",
    "rehype-sort-attributes": "^5.0.1",
    "settingz": "^0.2.0",
    "slash": "^5.1.0",
    "sort-keys": "^6.0.0",
    "stylelint": "^16.24.0",
    "stylelint-scss": "^6.12.1",
    "tsdown": "^0.15.2",
    "typescript": "~5.9.2",
    "yaml": "^2.8.1",
    "yarn-deduplicate": "^6.0.2"
  },
  "peerDependencies": {
    "eslint": "^9.35.0",
    "typescript": "~5.9.2"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "packageManager": "pnpm@10.16.1",
  "engines": {
    "node": "^20.18.0 || ^22.11.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "ava": {
    "timeout": "2m",
    "watchMode": {
      "ignoreChanges": [
        "**/temp/**"
      ]
    }
  },
  "nice-move": {
    "bundle": [
      "lib/**"
    ],
    "ignore": {
      "all": [
        "test/fixture/source/**"
      ],
      "eslint": [
        "test/helper/temp/**"
      ]
    }
  },
  "prettier": "@nice-move/all-in-web/prettier",
  "stylelint": {
    "extends": "@nice-move/all-in-web/stylelint"
  }
}
