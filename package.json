{
  "name": "garou",
  "version": "0.7.7",
  "description": "Do thing what heroes didn't do",
  "license": "MIT",
  "author": {
    "name": "Eric Chen",
    "email": "airkro@qq.com"
  },
  "keywords": [
    "css",
    "eslint",
    "format",
    "formatter",
    "garou",
    "html",
    "javascript",
    "markdown",
    "nice-move",
    "prettier",
    "react",
    "scss",
    "sort",
    "styleguide",
    "stylelint",
    "typescript",
    "vue"
  ],
  "homepage": "https://www.npmjs.com/package/garou",
  "repository": {
    "type": "git",
    "url": "https://github.com/nice-move/garou.git"
  },
  "bugs": {
    "url": "https://github.com/nice-move/garou/issues"
  },
  "bin": {
    "garou": "dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "best-shot prod",
    "garou": "node dist/cli.mjs",
    "lint:staged": "nice-move lint staged",
    "lint:type": "tsc --noEmit",
    "prepare": "nice-move git hooks",
    "prepublishOnly": "pnpm run lint:staged && pnpm test",
    "presnapshot": "pnpm run build",
    "snapshot": "ava --fail-fast -u -w",
    "pretest": "pnpm run build",
    "test": "ava --fail-fast"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^8.0.1",
    "@yarnpkg/lockfile": "^1.1.0",
    "eslint-module-utils": "^2.8.1",
    "flat-cache": "^3.2.0",
    "vue-eslint-parser": "^9.4.3",
    "write-file-atomic": "^5.0.1"
  },
  "devDependencies": {
    "@bring-it/cli": "^0.9.5",
    "@bring-it/npm": "^0.5.7",
    "@nice-move/all-in-base": "^0.1.6",
    "@nice-move/eslint-plugin-html": "0.0.0-beta.4",
    "@stylistic/stylelint-plugin": "^3.0.0",
    "@yarnpkg/lockfile": "^1.1.0",
    "ava": "^6.1.3",
    "best-shot": "^0.7.0",
    "cheetor": "^0.13.0",
    "eslint": "^8.57.0",
    "eslint-plugin-ava": "^14.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-class-members": "^1.20.0",
    "eslint-plugin-vue": "^9.27.0",
    "espree": "^9.6.1",
    "fs-chain": "^8.2.3",
    "globby": "^14.0.2",
    "micromatch": "^4.0.7",
    "object-traversal": "^1.0.1",
    "postcss": "^8.4.41",
    "postcss-html": "^1.7.0",
    "postcss-less": "^6.0.0",
    "postcss-markdown": "^1.2.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "rehype": "^13.0.1",
    "rehype-sort-attributes": "^5.0.0",
    "settingz": "^0.2.0",
    "slash": "^5.1.0",
    "sort-keys": "^5.0.0",
    "stylelint": "^16.8.1",
    "stylelint-scss": "~6.5.0",
    "typescript": "^5.5.4",
    "yaml": "^2.5.0",
    "yarn-deduplicate": "^6.0.2"
  },
  "peerDependencies": {
    "eslint": "^8.57.0 || ^9.0.0",
    "stylelint": "^16.8.1",
    "typescript": "^5.5.4"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "packageManager": "pnpm@9.5.0",
  "engines": {
    "node": "^18.12.0 || ^20.0.0 || ^22.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "ava": {
    "watchMode": {
      "ignoreChanges": [
        "**/temp/**"
      ]
    }
  },
  "eslintConfig": {
    "extends": "@nice-move/eslint-config-base"
  },
  "nice-move": {
    "bundle": [
      "lib/**"
    ],
    "ignore": {
      "all": [
        "test/fixture/source/**"
      ],
      "eslint": [
        "test/helper/temp/**"
      ]
    },
    "import-groups": "nice-move-preset"
  },
  "prettier": "@nice-move/prettier-config",
  "stylelint": {
    "extends": "@nice-move/stylelint-config"
  }
}
