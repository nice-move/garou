{
  "name": "garou",
  "version": "0.4.3",
  "description": "Do thing what heroes didn't do",
  "license": "MIT",
  "author": {
    "name": "Eric Chen",
    "email": "airkro@qq.com"
  },
  "keywords": [
    "css",
    "eslint",
    "format",
    "formatter",
    "garou",
    "html",
    "javascript",
    "markdown",
    "nice-move",
    "prettier",
    "react",
    "scss",
    "sort",
    "styleguide",
    "stylelint",
    "typescript",
    "vue"
  ],
  "homepage": "https://www.npmjs.com/package/garou",
  "repository": {
    "type": "git",
    "url": "https://github.com/nice-move/garou.git"
  },
  "bugs": {
    "url": "https://github.com/nice-move/garou/issues"
  },
  "bin": {
    "garou": "dist/cli.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "best-shot prod",
    "garou": "node dist/cli.cjs",
    "lint:staged": "nice-move lint staged",
    "prepare": "git config core.hooksPath .githooks",
    "prepublishOnly": "npm run lint:staged && npm run build && npm test",
    "test": "ava --fail-fast"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^5.14.0",
    "eslint-plugin-html": "^6.2.0",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@best-shot/cli": "^0.12.4",
    "@best-shot/core": "^0.6.6",
    "@nice-move/cli": "^0.7.0",
    "@nice-move/eslint-config-base": "^0.6.2",
    "@nice-move/prettier-config": "^0.5.2",
    "@nice-move/stylelint-config": "^0.6.14",
    "@yarnpkg/lockfile": "^1.1.0",
    "ava": "^4.1.0",
    "cheetor": "^0.12.5",
    "eslint": "^8.11.0",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-class-members": "^1.14.1",
    "eslint-plugin-vue": "^8.5.0",
    "espree": "^9.3.1",
    "fs-chain": "^8.1.2",
    "globby13": "npm:globby@13.1.1",
    "postcss": "^8.4.8",
    "postcss-html": "^1.3.0",
    "postcss-less": "^6.0.0",
    "postcss-markdown": "^1.2.0",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.5.1",
    "rehype": "^12.0.1",
    "rehype-sort-attributes": "^4.0.0",
    "settingz": "^0.1.7",
    "slash": "^4.0.0",
    "stylelint": "~14.3.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0",
    "typescript": "^4.6.2",
    "vue-eslint-parser": "^8.3.0",
    "webpack": "^5.70.0",
    "yarn-deduplicate": "^3.1.0"
  },
  "peerDependencies": {
    "eslint": "^8.11.0",
    "typescript": "^4.6.2"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": "^14.17.0 || >=16.13.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "eslintConfig": {
    "extends": "@nice-move/eslint-config-base",
    "rules": {
      "import/no-extraneous-dependencies": 0
    }
  },
  "nice-move": {
    "ignore": {
      "all": [
        "test/fixture/**"
      ],
      "eslint": [
        "test/helper/temp/**"
      ]
    }
  },
  "prettier": "@nice-move/prettier-config",
  "stylelint": {
    "extends": "@nice-move/stylelint-config"
  }
}