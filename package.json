{
  "name": "garou",
  "version": "0.9.4",
  "description": "Do thing what heroes didn't do",
  "license": "MIT",
  "author": {
    "name": "Eric Chen",
    "email": "airkro@qq.com"
  },
  "keywords": [
    "css",
    "eslint",
    "format",
    "formatter",
    "garou",
    "html",
    "javascript",
    "markdown",
    "nice-move",
    "prettier",
    "react",
    "scss",
    "sort",
    "styleguide",
    "stylelint",
    "typescript",
    "vue"
  ],
  "homepage": "https://www.npmjs.com/package/garou",
  "repository": {
    "type": "git",
    "url": "https://github.com/nice-move/garou.git"
  },
  "bugs": {
    "url": "https://github.com/nice-move/garou/issues"
  },
  "bin": {
    "garou": "dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "best-shot dev",
    "garou": "node dist/cli.mjs",
    "lint:staged": "nice-move lint staged",
    "lint:type": "tsc --noEmit",
    "prepare": "nice-move git hooks",
    "prepublishOnly": "pnpm run lint:staged && pnpm test",
    "presnapshot": "pnpm run build",
    "snapshot": "ava --fail-fast -u -w",
    "pretest": "pnpm run build",
    "test": "ava --fail-fast"
  },
  "dependencies": {
    "@stylistic/eslint-plugin-js": "^3.1.0",
    "@stylistic/eslint-plugin-jsx": "^3.1.0",
    "@stylistic/eslint-plugin-ts": "^3.1.0",
    "@typescript-eslint/parser": "^8.23.0",
    "@yarnpkg/lockfile": "^1.1.0",
    "eslint-module-utils": "^2.12.0",
    "vue-eslint-parser": "^9.4.3"
  },
  "devDependencies": {
    "@best-shot/analyzer": "^0.13.0",
    "@bring-it/cli": "^0.10.0",
    "@bring-it/npm": "^0.5.9",
    "@nice-move/all-in-base": "^0.4.10",
    "@nice-move/eslint-plugin-html": "0.1.1",
    "@rsdoctor/webpack-plugin": "^0.4.13",
    "@stylistic/stylelint-plugin": "^3.1.2",
    "@vue/compiler-sfc": "^3.5.13",
    "@yarnpkg/lockfile": "^1.1.0",
    "ava": "^6.2.0",
    "best-shot": "^0.9.0",
    "cheetor": "^0.13.0",
    "eslint": "^9.19.0",
    "eslint-plugin-ava": "^15.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-class-members": "^1.21.0",
    "eslint-plugin-vue": "^9.32.0",
    "espree": "^10.3.0",
    "fs-chain": "^8.2.4",
    "globby": "^14.1.0",
    "micromatch": "^4.0.8",
    "object-traversal": "^1.0.1",
    "postcss": "^8.5.1",
    "postcss-html": "^1.8.0",
    "postcss-less": "^6.0.0",
    "postcss-markdown": "^1.3.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.4.2",
    "rehype": "^13.0.2",
    "rehype-sort-attributes": "^5.0.1",
    "settingz": "^0.2.0",
    "slash": "^5.1.0",
    "sort-keys": "^5.1.0",
    "stylelint": "^16.14.1",
    "stylelint-scss": "^6.11.0",
    "typescript": "~5.7.3",
    "yaml": "^2.7.0",
    "yarn-deduplicate": "^6.0.2"
  },
  "peerDependencies": {
    "eslint": "^9.19.0",
    "typescript": "^5.7.3"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "packageManager": "pnpm@9.15.5",
  "engines": {
    "node": "^20.18.0 || ^22.11.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "ava": {
    "watchMode": {
      "ignoreChanges": [
        "**/temp/**"
      ]
    }
  },
  "nice-move": {
    "bundle": [
      "lib/**"
    ],
    "ignore": {
      "all": [
        "test/fixture/source/**"
      ],
      "eslint": [
        "test/helper/temp/**"
      ]
    }
  },
  "prettier": "@nice-move/prettier-config",
  "stylelint": {
    "extends": "@nice-move/stylelint-config"
  }
}
