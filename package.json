{
  "name": "garou",
  "version": "0.6.13",
  "description": "Do thing what heroes didn't do",
  "license": "MIT",
  "author": {
    "name": "Eric Chen",
    "email": "airkro@qq.com"
  },
  "keywords": [
    "css",
    "eslint",
    "format",
    "formatter",
    "garou",
    "html",
    "javascript",
    "markdown",
    "nice-move",
    "prettier",
    "react",
    "scss",
    "sort",
    "styleguide",
    "stylelint",
    "typescript",
    "vue"
  ],
  "homepage": "https://www.npmjs.com/package/garou",
  "repository": {
    "type": "git",
    "url": "https://github.com/nice-move/garou.git"
  },
  "bugs": {
    "url": "https://github.com/nice-move/garou/issues"
  },
  "bin": {
    "garou": "dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "best-shot prod",
    "garou": "node dist/cli.mjs",
    "lint:staged": "nice-move lint staged",
    "lint:type": "tsc --noEmit",
    "prepare": "nice-move git hooks",
    "prepublishOnly": "npm run lint:staged && npm test",
    "snapshot": "ava --fail-fast -u",
    "pretest": "npm run build",
    "test": "ava --fail-fast"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^6.5.0",
    "@yarnpkg/lockfile": "^1.1.0",
    "eslint-module-utils": "^2.8.0",
    "flat-cache": "^3.1.0",
    "vue-eslint-parser": "^9.3.1",
    "write-file-atomic": "^5.0.1"
  },
  "devDependencies": {
    "@bring-it/npm": "^0.2.10",
    "@nice-move/cli": "^0.10.11",
    "@nice-move/eslint-config-base": "^0.9.19",
    "@nice-move/eslint-plugin-html": "0.0.0-beta.3",
    "@nice-move/prettier-config": "^0.9.3",
    "@nice-move/stylelint-config": "^0.9.5",
    "@nice-move/tsconfig": "^0.1.0",
    "@yarnpkg/lockfile": "^1.1.0",
    "ava": "^5.3.1",
    "best-shot": "^0.4.1",
    "cheetor": "^0.13.0",
    "eslint": "^8.48.0",
    "eslint-plugin-ava": "^14.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-class-members": "^1.18.0",
    "eslint-plugin-vue": "^9.17.0",
    "espree": "^9.6.1",
    "fs-chain": "^8.2.2",
    "globby": "^13.2.2",
    "micromatch": "^4.0.5",
    "object-traversal": "^1.0.1",
    "postcss": "^8.4.29",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "postcss-markdown": "^1.2.0",
    "postcss-scss": "^4.0.7",
    "prettier": "^3.0.3",
    "rehype": "^13.0.1",
    "rehype-sort-attributes": "^5.0.0",
    "settingz": "^0.2.0",
    "slash": "^5.1.0",
    "stylelint": "^15.10.3",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "~5.1.0",
    "typescript": "^5.2.2",
    "yaml": "^2.3.2",
    "yarn-deduplicate": "^6.0.2"
  },
  "peerDependencies": {
    "eslint": "^8.45.0",
    "typescript": "^4.9.5 || ^5.1.6"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=19.0.0 || ^16.15.0 || ^18.12.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "eslintConfig": {
    "extends": "@nice-move/eslint-config-base"
  },
  "nice-move": {
    "bundle": [
      "lib/**"
    ],
    "ignore": {
      "all": [
        "test/fixture/**"
      ],
      "eslint": [
        "test/helper/temp/**"
      ]
    },
    "import-groups": "nice-move-preset"
  },
  "prettier": "@nice-move/prettier-config",
  "stylelint": {
    "extends": "@nice-move/stylelint-config"
  }
}
